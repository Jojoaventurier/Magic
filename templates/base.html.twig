<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>sf</text></svg>">
        {% block stylesheets %}{# <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js" integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script> #}
        <link rel="stylesheet" href="{{ asset('styles/app.css') }}"
        {% endblock %}

        {% block javascripts %}
        {% block importmap %}{% endblock %}
        {% endblock %}

          {% if app.user %}
            <a href="{{ path('app_logout') }}">Se déconnecter</a>
            <a href="{{ path('app_profile') }}">Mon profil</a>
          {% else %}
            <a href="{{ path('app_login') }}">Se connecter</a>
            <a href="{{ path('app_register') }}">S'inscrire</a>
          {% endif %}
          <a href="{{ path('app_forum') }}">Forum</a>

        {% block body %}{% endblock %}
    </body>
</html>
