{% extends 'base.html.twig' %}

{% block title %}Liste d'utilisateurs{% endblock %}

{% block body %}


<h2>Select a user to chat with</h2>

<ul>
    {% for user in users %}
        <li>
            <a href="{{ path('chatting', { 'id': user.id }) }}">
                {{ user.userName }}
            </a>
        </li>
    {% endfor %}
</ul>

{% block javascripts %}
{% endblock %}
{% endblock %}